game:GetService("StarterGui"):SetCore("SendNotification",{["Title"]="$",["Text"]="Press T and stand still (its so you wont be able to be hit but you cant shoot tho unless you move) ",["Duration"]=20,["Button1"]="okay"})game:GetService("StarterGui"):SetCore("SendNotification",{["Title"]="$",["Text"]="Keybind for lock is Q",["Duration"]=20,["Button1"]="okay"})game:GetService("StarterGui"):SetCore("SendNotification",{["Title"]="$",["Text"]="Enjoy.",["Duration"]=20,["Button1"]="!"})local a="t"checkcaller=checkcaller;newcclosure=newcclosure;hookmetamethod=hookmetamethod;local b=false;local c=game:GetService("RunService")local d=game:GetService("Players").LocalPlayer;local e=d:GetMouse()e.KeyDown:Connect(function(f)if f==string.lower(a)then pcall(function()if b==false then b=true;print("Enabled P1000")elseif b==true then b=false;print("Disabled P1000")end end)end end)e.KeyDown:Connect(function(f)if f=="="then game:GetService("TeleportService"):Teleport(game.PlaceId,d)end end)function RandomNumberRange(g)return math.random(-g*100,g*100)/100 end;function RandomVectorRange(g,h,i)return Vector3.new(RandomNumberRange(g),RandomNumberRange(h),RandomNumberRange(i))end;local j={}c.Heartbeat:Connect(function()if b==true then j[1]=d.Character.HumanoidRootPart.CFrame;j[2]=d.Character.HumanoidRootPart.AssemblyLinearVelocity;local k=d.Character.HumanoidRootPart.CFrame;k=k*CFrame.new(Vector3.new(0,0,0))k=k*CFrame.Angles(math.rad(RandomNumberRange(180)),math.rad(RandomNumberRange(180)),math.rad(RandomNumberRange(180)))d.Character.HumanoidRootPart.CFrame=k;d.Character.HumanoidRootPart.AssemblyLinearVelocity=Vector3.new(1,1,1)*16384;c.RenderStepped:Wait()d.Character.HumanoidRootPart.CFrame=j[1]d.Character.HumanoidRootPart.AssemblyLinearVelocity=j[2]end end)local l=nil;l=hookmetamethod(game,"__index",newcclosure(function(self,m)if b==true then if not checkcaller()then if m=="CFrame"and b==true and d.Character and d.Character:FindFirstChild("HumanoidRootPart")and d.Character:FindFirstChild("Humanoid")and d.Character:FindFirstChild("Humanoid").Health>0 then if self==d.Character.HumanoidRootPart then return j[1]or CFrame.new()elseif self==d.Character.Head then return j[1]and j[1]+Vector3.new(0,d.Character.HumanoidRootPart.Size/2+0.5,0)or CFrame.new()end end end end;return l(self,m)end))
